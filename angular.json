{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "",
    "projects": {
        "stepflow": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/stepflow",
            "sourceRoot": "apps/stepflow/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/stepflow",
                        "index": "apps/stepflow/src/index.html",
                        "main": "apps/stepflow/src/main.ts",
                        "polyfills": "apps/stepflow/src/polyfills.ts",
                        "tsConfig": "apps/stepflow/tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "apps/stepflow/src/favicon.ico",
                            "apps/stepflow/src/assets"
                        ],
                        "styles": [
                            "node_modules/primeng/resources/themes/saga-blue/theme.css",
                            "node_modules/primeng/resources/primeng.min.css",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeflex/primeflex.css",
                            "node_modules/quill/dist/quill.core.css",
                            "node_modules/quill/dist/quill.snow.css",
                            "apps/stepflow/src/sass/styles.scss"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "apps/stepflow/src/sass"
                            ]
                        },
                        "scripts": [
                            "node_modules/quill/dist/quill.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/stepflow/src/environments/environment.ts",
                                    "with": "apps/stepflow/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "stepflow:build",
                        "port": 4200,
                        "proxyConfig": "apps/stepflow/src/proxy.conf.json"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "stepflow:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "stepflow:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/stepflow/tsconfig.app.json",
                            "apps/stepflow/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "stepflow-e2e": {
            "projectType": "application",
            "root": "apps/stepflow-e2e",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "apps/stepflow-e2e/protractor.conf.js",
                        "devServerTarget": "stepflow:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "stepflow:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "apps/stepflow-e2e/tsconfig.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "stepflow-backend": {
            "root": "apps/stepflow-backend",
            "sourceRoot": "apps/stepflow-backend/src",
            "projectType": "application",
            "prefix": "stepflow-backend",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "options": {
                        "outputPath": "dist/apps/stepflow-backend",
                        "main": "apps/stepflow-backend/src/main.ts",
                        "tsConfig": "apps/stepflow-backend/tsconfig.app.json",
                        "assets": [
                            "apps/stepflow-backend/src/assets"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/stepflow-backend/src/environments/environment.ts",
                                    "with": "apps/stepflow-backend/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "port": 8085,
                        "buildTarget": "stepflow-backend:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/stepflow-backend/tsconfig.app.json",
                            "apps/stepflow-backend/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/stepflow-backend/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/stepflow-backend/jest.config.js",
                        "tsConfig": "apps/stepflow-backend/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "shared-interfaces": {
            "root": "libs/shared/interfaces",
            "sourceRoot": "libs/shared/interfaces/src",
            "projectType": "library",
            "schematics": {},
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/shared/interfaces/tsconfig.lib.json",
                            "libs/shared/interfaces/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!libs/shared/interfaces/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/shared/interfaces/jest.config.js",
                        "tsConfig": "libs/shared/interfaces/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "defaultProject": "stepflow",
    "cli": {
        "defaultCollection": "@nrwl/nest"
    }
}
