{
  "extends": "gts/tslint.json",
  "rules": {
    "member-access": false,
    "nx-enforce-module-boundaries": [
      true,
      {
        "allow": [],
        "depConstraints": [
          {
            "sourceTag": "*",
            "onlyDependOnLibsWithTags": [
              "*"
            ]
          }
        ]
      }
    ]
  },
  "linterOptions": {
    "exclude": [
      "**/*.json"
    ]
  },
  "rulesDirectory": [
    "node_modules/@nrwl/workspace/src/tslint"
  ]
}
