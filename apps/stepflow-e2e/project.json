{
    "projectType": "application",
    "root": "apps/stepflow-e2e",
    "targets": {
        "e2e": {
            "executor": "@nrwl/build-angular:protractor",
            "options": {
                "protractorConfig": "apps/stepflow-e2e/protractor.conf.js",
                "devServerTarget": "stepflow:serve"
            },
            "configurations": {
                "production": {
                    "devServerTarget": "stepflow:serve:production"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/build-angular:tslint",
            "options": {
                "tsConfig": "apps/stepflow-e2e/tsconfig.json",
                "exclude": ["**/node_modules/**"]
            }
        }
    }
}
